<UserControl x:Class="psychotest.View.SummaryView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:psychotest.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <TextBlock TextWrapping="Wrap" Text="Dziękujemy za wzięcie udziału w badaniu." TextAlignment="Center" FontSize="20" FontFamily="Microsoft YaHei" Background="#FF475BCA" Foreground="White" Height="35"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="50">
                <Button Content="Wykonaj ponownie" Margin="0,0,50,0" VerticalAlignment="Bottom" Command="{Binding GoToHelloView}" Width="150"/>
                <Button Content="Zakończ"  Width="150" Command="{Binding CloseProgram}" />
            </StackPanel>

           
        </StackPanel>
        
    </Grid>
</UserControl>
